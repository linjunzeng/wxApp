page{background: #f1f1f1;}
.list{margin-bottom: 24rpx;padding: 20rpx;background-color: #fff;box-sizing: border-box;}
.list .log_title{margin:0 32rpx;padding:20rpx 0;color:#666;font-size:27rpx;border-bottom:1px solid #eee;}
.list .img{width: 165rpx;height: 165rpx;position: relative;overflow: hidden;}
.list .img img{margin:auto;max-width:100%;max-height:100%;position:absolute;left:0;top:0;right:0;bottom:0;}
.list .img .number{width: 100%;height: 39rpx;line-height: 39rpx;text-align: center;font-size: 20rpx;background: rgba(0,0,0,0.4);color: #fff;position: absolute;left: 0;bottom: 0;z-index: 9999;}
.list .info{padding: 0 20rpx;height: 165rpx;line-height: 56rpx;overflow:hidden;box-sizing: border-box;}
.list .info span{color: #e41b45;}